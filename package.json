{
  "name": "@frontender-magazine/articles-list",
  "version": "1.0.0",
  "description": "Index page of magazine",
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./lib/server",
    "release": "npm version patch && git push --tags && npm publish --access public",
    "clean": "rimraf public lib",
    "build": "npm-run-all build:*",
    "build:js": "npm run clean && babel source/server.js --minified -s true -d lib --ignore __tests__/**,__mocks__/** ",
    "build:css": "gulp css",
    "documentation": "documentation build source/*.js* -o public -f html",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext .js --color -f stylish --fix ./source",
    "lint:css": "stylelint './source/**/*.css' --color -f verbose --fix",
    "minify": "babel-minify lib/*.js -d lib --removeConsole --removeDebugger",
    "precommit": "git add . -A && lint-staged",
    "commitmsg": "commit-msg ${GIT_PARAMS}"
  },
  "lint-staged": {
    "*.js": [
      "eslint --color -f stylish --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --color -f verbose --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:TemplateMonster/PlasmaPlatform/Frontend/tm-amazon-rename.git"
  },
  "keywords": [
    "fm",
    "amazon",
    "bucket",
    "replacer"
  ],
  "author": "Anton Nemtsev <thesilentimp@gmail.com> (http://silentimp.info/)",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "@frontender-magazine/models": "^1.1.0",
    "dotenv": "^7.0.0",
    "ejs": "^2.5.7",
    "intl": "^1.2.5",
    "intl-relativeformat": "^2.1.0",
    "moment": "^2.19.2",
    "mongoose": "^5.4.20",
    "path": "^0.12.7",
    "query-string": "^6.4.2",
    "restify": "^8.2.0",
    "restify-cookies": "^0.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@plasma-platform/tm-message-hook": "^2.0.14",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "documentation": "^9.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.0",
    "gulp-order": "^1.1.1",
    "gulp-postcss": "^8.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.1",
    "postcss-conditionals": "^2.1.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.2",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.5.4"
  }
}
