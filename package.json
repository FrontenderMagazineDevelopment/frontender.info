{
  "name": "@frontender-magazine/articles-list",
  "version": "1.0.0",
  "description": "Index page of magazine",
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./lib/server",
    "release": "npm version patch && git push --tags && npm publish --access public",
    "clean": "rimraf public lib",
    "build": "npm-run-all build:*",
    "build:js": "npm run clean && babel source --minified -s true -d lib --ignore __tests__/**,__mocks__/** ",
    "build:css": "gulp css:common",
    "build:doc": "documentation build src/*.js* -o public -f html",
    "test": "export NODE_ENV=test && jest --no-cache",
    "test:watch": "export NODE_ENV=test && jest --no-cache --watch",
    "test:coverage": "export NODE_ENV=test && jest --no-cache --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint ./source/** --ext .jsx,.js --color -f stylish --fix",
    "lint:doc": "documentation lint source/**/*.js*",
    "lint:css": "stylelint './source/**/*.css' --color -r -f verbose --fix",
    "minify": "babel-minify lib/*.js -d lib --removeConsole --removeDebugger",
    "precommit": "git add . -A && lint-staged",
    "commitmsg": "commit-msg ${GIT_PARAMS}"
  },
  "lint-staged": {
    "*.js": [
      "eslint --color -f stylish --fix",
      "git add . -A",
      "jest --bail --findRelatedTests"
    ],
    "*.css": [
      "stylelint --color -r -f verbose --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:TemplateMonster/PlasmaPlatform/Frontend/tm-amazon-rename.git"
  },
  "keywords": [
    "tm",
    "amazon",
    "bucket",
    "replacer"
  ],
  "author": "Anton Nemtsev <thesilentimp@gmail.com> (http://silentimp.info/)",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@frontender-magazine/models": "^1.0.1",
    "@plasma-platform/tm-message-hook": "^2.0.5",
    "babel-polyfill": "^6.26.0",
    "documentation": "^5.1.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "intl": "^1.2.5",
    "intl-relativeformat": "^2.1.0",
    "moment": "^2.19.2",
    "mongoose": "^4.13.5",
    "query-string": "^5.0.1",
    "restify": "^6.3.4",
    "restify-cookies": "^0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.0",
    "gulp-order": "^1.1.1",
    "gulp-postcss": "^7.0.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^5.0.0",
    "nock": "^9.0.14",
    "npm-run-all": "^4.1.2",
    "postcss-conditionals": "^2.1.0",
    "postcss-import": "^11.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^2.1.2",
    "prettier": "^1.8.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-test-renderer": "^16.1.1",
    "rimraf": "^2.6.2",
    "stylelint": "^8.3.1",
    "stylelint-scss": "^2.1.0"
  }
}
